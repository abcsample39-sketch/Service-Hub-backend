{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/apps/service-hub-backend",
  "installCommand": "pnpm install --frozen-lockfile",
  "functions": {
    "dist/apps/service-hub-backend/main.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30,
      "includeFiles": ["dist/apps/service-hub-backend/**"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/service-hub-backend/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
